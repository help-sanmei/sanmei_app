<%= form_for(@team, html:{class: 'form-horizontal'}) do |f| %>
  <table class="table table-striped table-bordered table-hover">
          <tbody>
            <tr>
               <th>苗字</th>
               <th>名前</th>
               <th>生年月日</th>
               <th>性別</th>
               <th>Action</th>
            </tr>
            <%= f.nested_fields_for :staffs, wrapper_tag: :tr do |q| %>
                <td><%= q.text_field :family_name, class: 'form-control' %></td>
                <td><%= q.text_field :first_name, class: 'form-control' %></td>
                <td><%= q.date_field :birthday, class: 'form-control' %></td>
                <td><%= q.text_field :gender, class: 'form-control' %></td>
                <td><%= q.remove_nested_fields_link 'Delete', class: 'btn btn-danger', role: 'button' %></td>
            <% end %>
        </tbody>
      </table>
      <%= f.add_nested_fields_link :staffs, 'Add new', class: 'btn btn-primary', role: 'button' %>

<% end %>